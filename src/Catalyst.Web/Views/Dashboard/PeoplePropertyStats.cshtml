@model Catalyst.Web.Models.Dashboard.PeoplePropertyStats
@{
    Layout = "_DashboardItem.cshtml";
}
@if (Model.Metrics.Any())
{
<ul class="list-group">
@foreach (var metric in Model.Metrics)
{
    <li class="list-group-item justify-content-between">
        @metric.Name
        <span class="badge badge-default badge-pill">@metric.Count</span>
    </li>
}
</ul>
}